{
  "name": "vscode-node-readme",
  "displayName": "node-readme",
  "description": "view installed node_modules readmes",
  "version": "1.0.7",
  "publisher": "bengreenier",
  "icon": "images/icon.svg",
  "engines": {
    "vscode": "^1.0.0"
  },
  "galleryBanner": {
    "color": "#ffc985",
    "theme": "light"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:nodeReadme.showReadme"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "node-readme configuration",
      "properties": {
        "node-readme.moduleMappings": {
          "type": "object",
          "default": {},
          "description": "A mapping of any module name, to it's documentation file (in markdown) that overrides the default documentation."
        }
      }
    },
    "menus": {
      "editor/context": [
        {
          "when": "editorLangId == javascript",
          "command": "nodeReadme.showReadme",
          "group": "navigation"
        },
        {
          "when": "editorLangId == typescript",
          "command": "nodeReadme.showReadme",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "title": "View Node Module Readme",
        "command": "nodeReadme.showReadme"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "node-core-module-names": "^1.1.0",
    "request": "^2.74.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bengreenier/vscode-node-readme.git"
  },
  "keywords": [
    "vscode",
    "node",
    "readme"
  ],
  "author": "Ben Greenier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bengreenier/vscode-node-readme/issues"
  },
  "homepage": "https://github.com/bengreenier/vscode-node-readme#readme"
}
